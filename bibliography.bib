  keywords = {Assessment,confirmatory factor-analysis,Depression,Generalized anxiety disorder,health,Internet,Internet   administration,ls2,model,Penn State Worry Questionnaire,psychometric   properties,Reliability,tests,Validity,web,worry},
  pages = {844--848},
  file = {verkuil_brosschot_2012.pdf:/home/paul/.zotero/zotero/x98kwiq0.default/zotero/storage/X7QJUME4/verkuil_brosschot_2012.pdf:application/pdf}

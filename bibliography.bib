  file = {PubMed Central Full Text PDF:/home/paul/.zotero/zotero/x98kwiq0.default/zotero/storage/WBVE88C8/LeMoult et al. - 2013 - State Rumination Associations with Emotional Stre.pdf:application/pdf},

  file = {cuijpers_2015.pdf:/home/paul/.zotero/zotero/x98kwiq0.default/zotero/storage/QTIAXU8N/cuijpers_2015.pdf:application/pdf}
}

@article{andrews_internet_2014,
  title = {{INTERNET} {PSYCHOTHERAPY} {AND} {THE} {FUTURE} {OF} {PERSONALIZED} {TREATMENT}},

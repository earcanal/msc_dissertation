  file = {Snapshot:/home/paul/.zotero/zotero/x98kwiq0.default/zotero/storage/KV5DFJCU/A1023910315561.html:text/html;treynor_et_al_2003.pdf:/home/paul/.zotero/zotero/x98kwiq0.default/zotero/storage/KIXTXGHV/treynor_et_al_2003.pdf:application/pdf}

  keywords = {applied relaxation,Attention,computerized,Depression,dissemination,Generalized anxiety disorder,Internet,ls2,primary-care,randomized controlled-trial,rating-scale,selective attention,self-help,social phobia,state worry questionnaire,treatment},
  pages = {546--559},
  file = {amir_taylor_2012.pdf:/home/paul/.zotero/zotero/x98kwiq0.default/zotero/storage/EGN2ARKF/amir_taylor_2012.pdf:application/pdf}

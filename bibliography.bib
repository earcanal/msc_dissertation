  number = {1},
  urldate = {2015-07-13},
  journal = {Journal of Clinical Epidemiology},
  author = {Wahl, Inka and L{\"o}we, Bernd and Bjorner, Jakob Bue and Fischer, Felix and Langs, Gernot and Voderholzer, Ulrich and Aita, Stephen A. and Bergemann, Niels and Br{\"a}hler, Elmar and Rose, Matthias},
  month = jan,
